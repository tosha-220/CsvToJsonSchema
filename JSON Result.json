{
  "$schema":"http://json-schema.org/draft-04/schema#",
  "type":"object",
  "description":"Modify CE Service Request",
  "properties":{
    "ce_parameters":{
      "type":"object",
      "description":"Object contains CE info  o-50-8",
      "properties":{
        "ce_line_structure_type":{
          "type":"string",
          "enum":[
            "Act-Act",
            "Act-Sby"
          ],
          "description":"Dual line type 15-1-181"
        },
        "ce_active_line_selection":{
          "type":"string",
          "enum":[
            "INET",
            "MPLS",
          ],
          "description":"CE active line selection 15-1-182"
        },
        "ce_terminal_apply_cold_standby":{
          "type":"boolean",
          "description":"Cold standby necessity 15-1-4"
        },
        "ce_terminal_install_address_country":{
          "type":"string",
          "pattern":"^.*$",
          "description":"Country name 15-1-8"
        },
        "internet_breakout_microsoft_status":{
          "type":"boolean",
          "description":"MS Office 365 Internet Brakeout, 16-10-1"
        },
        "ce_internet_breakout_custom": {
          "type":"array",
          "description":"Array custom ip addresses o-50-50",
          "items": {
            "type":"object",
            "properties": {
              "ce_internet_breakout_custom_list_id": {
                "type":"string",
                "description":"Unique ID for custom subnet generated by BE, 16-4-5"
              },
              "ce_internet_breakout_custom_management_name": {
                "type":"string",
                "description":"CE internet breakout custom management name, 16-4-2"
              },
              "ce_internet_breakout_custom_network_ipaddr": {
                "type":"string",
                "pattern":"^([1-9]|[1-9]\\d|1\\d{2}|2[0-4]\\d|25[0-5])(\\.(\\d|[1-9]\\d|1\\d{2}|2[0-4]\\d|25[0-5])){3}$",
                "description":"CE internet breakout custom network ip address, 16-4-3"
              },
              "ce_internet_breakout_custom_network_subnet": {
                "type":"integer",
                "minimum": 0,
                "maximum": 32,
                "exclusiveMinimum": true,
                "description":"CE internet breakout custom network subnet, 16-4-4"
              }
            }
          }
          "required": [
            "ce_internet_breakout_custom_management_name",
            "ce_internet_breakout_custom_network_ipaddr",
            "ce_internet_breakout_custom_network_subnet"
          ]
        },
        "required": [
          "ce_line_structure_type",
          "ce_active_line_selection"
        ]
      },
      "ce_parameters_secondary":{
        "type":"object",
        "description":"Object contains Secondary CE info,  o-50-44",
        "properties":{
          "ce_gw_name":{
            "type":"string",
            "pattern":"^.{1,32}$",
            "description":"Name 15-1-301"
          },
          "ce_status":{
            "type":"string",
            "enum":[
              "Processing",
              "Kitting",
              "Delivered",
              "Activated",
              "WaitingSendBack",
              "SentBack"
            ],
            "description":"Status 15-1-318"
          },
          "required":[
            "ce_gw_name"
          ]
        },
        "required":[
          "ce_parameters"
        ]
      }

